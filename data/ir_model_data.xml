        <field name="name">Brevo Synchronization Log</field>
        <field name="model">brevo.sync.log</field>
        <field name="state">manual</field>
    </record>

    <record id="model_brevo_config_wizard" model="ir.model">
        <field name="name">Brevo Configuration Wizard</field>
        <field name="model">brevo.config.wizard</field>
        <field name="state">manual</field>
    </record>
