<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Model Definitions -->
    <record id="model_brevo_config" model="ir.model">
        <field name="name">Brevo Configuration</field>
        <field name="model">brevo.config</field>
        <field name="state">manual</field>
    </record>

    <record id="model_brevo_contact_list" model="ir.model">
        <field name="name">Brevo Contact List</field>
        <field name="model">brevo.contact.list</field>
        <field name="state">manual</field>
